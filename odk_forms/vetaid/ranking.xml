				</xf:item>
				<xf:item>
					<xf:label>5</xf:label>
					<xf:value>5</xf:value>
				</xf:item>
				<xf:item>
					<xf:label>6</xf:label>
					<xf:value>6</xf:value>
				</xf:item>
				<xf:item>
					<xf:label>7</xf:label>
