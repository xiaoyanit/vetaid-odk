<xf:html xmlns:xf="http://www.w3.org/2002/xforms"
	xmlns:ev="http://www.w3.org/2001/xml-events"
	xmlns:jr="http://openrosa.org/javarosa">
	
  <head>
    <title>Farm Survey</title>

	<!-- XForm Data Model -->
	<xf:model id="farm">
			<xf:instance id="mainInstance">
				<farm>
					<enterprise/>
					<livestock>
						<totals>
							<cattle>0</cattle>
							<goats>0</goats>
							<pigs>0</pigs>
							<sheep>0</sheep>
							<chickens>0</chickens>
							<cattle_type/>
						</totals>
						<management>
							<grazing_type/>
							<herding_responsibility/>
							<cattle_mixing/>
							<buffalo_mixing/>
							<calves_born_this_year/>
							<calves_and_adults_grazing_together/>
							<cattle_purpose_rank_1/>
							<cattle_purpose_rank_2/>
							<cattle_purpose_rank_3/>
							<cattle_purpose_rank_4/>
							<cattle_purpose_rank_5/>
							<cattle_purpose_rank_6/>
							<cattle_purpose_rank_7/>
							<cattle_purpose_rank_8/>																																																	
						</management>							
					</livestock>
					<cattle_health_management>	
						<sick_animal_carer/>	
						
						<diseases_affecting_cattle/>
						<animals_affected_in_year/>
						<deaths_in_year/>
						<treatments_used/>
						<source_of_advice_recieved/>
						<when_will_seek_assistance/>
						<where_will_seek_assistance/>						
						<deaths_aged_zero_to_one_month/>
						<deaths_aged_one_to_four_months/>
						<deaths_aged_four_to_twelve_months/>
						<deaths_aged_more_than_twelve_months/>
						<top_rated_desired_improvements/>
						
						<purpose_of_cattle_with_higher_survival_rate/>
						<sell_cattle_to_whom/>
						<associates_ticks_with_disease_in_cattle/>
						<disease_associated_with_ticks1/>
						<disease_associated_with_ticks2/>
						<disease_associated_with_ticks3/>
						
						
					</cattle_health_management>
  				</farm>				
				
			</xf:instance>
			
			<!-- Farm Details -->
			<xf:bind id="enterprise" nodeset="/farm/enterprise" type="string"/>
			<xf:bind id="noOfCattle" nodeset="/farm/livestock/totals/cattle" required="true()" type="integer"/>
			<xf:bind id="noOfGoats" nodeset="/farm/livestock/totals/goats" required="true()" type="integer"/>	

			<xf:bind id="noOfPigs" nodeset="/farm/livestock/totals/pigs" required="true()" type="integer"/>	
